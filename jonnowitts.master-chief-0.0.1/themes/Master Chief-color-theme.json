			"settings": {
				//"foreground": "#5cf29a"
				"foreground": "#45C0F2"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				//"foreground": "#f9ffe3" // green
				"foreground": "#96A65D"
				//"foreground": "#ECB455" // gold
				//"foreground": "#CED971"
				//"foreground": "#65a4b6",// blue
				//"foreground": "#a76933" // brown/bronze
			}
		},
		{
			"name": "Other Variable, String Link", // Link in markdown
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#8bbad3"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded", // &copy; in HTML
