	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#628EA0"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#FFC35C"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#E45E5E"
			}
		},
		{
			"name": "Invalid", // when error occurs
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E45E5E"
			}
		},
		{
			"name": "Keyword, Storage", // var, function etc
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#379AD9"
			}
		},
		{
			"name": "Operator, Misc", // Chevrons, P {}
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#C6C6C6"
			}
		},
		{
			"name": "Keyword", // Chevrons, keywords, P {}
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#57E3F2"
			}
		},
		{
			"name": "Tag", // HTML Tags
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#7F8C50"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
